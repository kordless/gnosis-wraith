# Test Environment Configuration
# Copy this file to .env and fill in your API keys

# IMPORTANT: Gnosis Wraith API Token
# Get this from the web UI after logging in:
# 1. Go to http://localhost:5000
# 2. Log in with your phone number
# 3. Click on the API Token button in the UI
# 4. Copy your token here
GNOSIS_API_TOKEN=your-gnosis-wraith-api-token-here

# OpenAI API Key (for GPT models)
OPENAI_API_KEY=sk-your-openai-api-key-here

# Anthropic API Key (for Claude models)
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key-here

# Google AI API Key (for Gemini models)
GOOGLE_API_KEY=your-google-api-key-here

# Ollama Settings (for local models)
OLLAMA_BASE_URL=http://localhost:11434

# Test Configuration
TEST_BASE_URL=http://localhost:5678
TEST_TIMEOUT=30
TEST_USER_EMAIL=test@example.com
TEST_USER_PHONE=+1234567890

# Feature Flags
ENABLE_SCREENSHOT_TESTS=false
ENABLE_JAVASCRIPT_TESTS=true
ENABLE_AI_TESTS=true